        {
            "name": "Derek Illchuk",
            "email": "derek@illchuk.com",
            "homepage": "https://www.beaucal.com"
        }
    ],
    "require": {
        "php": "^5.3.3 || ^7.0",
        "zendframework/zend-session" : "^2.1",
		"zendframework/zend-mvc" : "^2.1 || ^3.0"
    },
    "autoload": {
